                                    <ColumnDefinition Width="40" />
                                    <ColumnDefinition Width="70" />
                                    <ColumnDefinition Width="100" />
                                    <ColumnDefinition Width="100" />
                                    <ColumnDefinition Width="100" />
                                    <ColumnDefinition Width="100" />
                                    <ColumnDefinition Width="80" />
                                </Grid.ColumnDefinitions>





                                <Image
                                    Grid.Column="0"
                                    Width="20"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Bottom"
                                    Source="{Binding PAID}"
                                    Stretch="Fill" />
                                <TextBlock
                                    x:Name="XXX"
                                    Grid.Column="1"
                                    Text="{Binding ID}" />
                                <Image
                                    Grid.Column="2"
                                    Width="20"
                                    Margin="0,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Bottom"
                                    Source="{Binding FLAG}"
                                    Stretch="Fill" />
                                <TextBlock Grid.Column="3" Text="{Binding COUNTRY}" />
                                <TextBlock
                                    Grid.Column="4"
                                    HorizontalAlignment="left"
                                    Text="{Binding FIRSTNAME}" />
                                <TextBlock Grid.Column="5" Text="{Binding LASTNAME}" />
                                <TextBlock Grid.Column="6" Text="{Binding FAILIC}" />
                                <TextBlock Grid.Column="7" Text="{Binding NACLIC}" />
                                <TextBlock Grid.Column="8" Text="{Binding AGECAT}" />
                            </Grid>
                        </DataTemplate>
                    </ListBox.ItemTemplate>
